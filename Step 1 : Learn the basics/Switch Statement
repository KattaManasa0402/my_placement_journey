class Solution {
  public:
    double switchCase(int choice, vector<double> &arr) {
        switch(choice){
            case 1:{
            double area1=M_PI*arr[0]*arr[0];
            return area1;
            }
            case 2:{
            double area2=arr[0]*arr[1];
            return area2;
            }
            default:
            break;
        }
    }
};
