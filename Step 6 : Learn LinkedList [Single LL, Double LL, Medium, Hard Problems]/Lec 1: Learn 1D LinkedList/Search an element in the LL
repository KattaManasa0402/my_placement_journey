class Solution {
  public:
    // Function to count nodes of a linked list.
    bool searchKey(int n, Node* head, int key) {
        Node* temp=head;
        while(temp!=NULL){
        if(temp->data==key){
            return true;
        }
        else{
            temp=temp->next;
        }
        }
        return false;
        
    }
};
