// solved on leetcode 
class MyStack {
    queue<int>q1;
    queue<int>q2;
public:
    MyStack(){
    }
    void push(int x) {
        for(int i=0;i<q1.size();i++){
            q2.push(q1.front());   
            q1.pop();
        }
        q1.push(x);
        for(int i=0;i<q2.size();i++){
            q1.push(q2.front());
            q2.pop();
        }
    }
    
    int pop() {
        int el= q1.front();
        q1.pop();
        return el;
    }
    
    int top() {
        return q1.front();
    }
    
    bool empty() {
        return(q1.size()==0);
    }
};

/**
 * Your MyStack object will be instantiated and called as such:
 * MyStack* obj = new MyStack();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->top();
 * bool param_4 = obj->empty();
 */
